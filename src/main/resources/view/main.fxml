<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.aion.gui.controller.main">
    <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <TextArea fx:id="solutions" prefHeight="200.0" prefWidth="200.0" promptText="Solutions" GridPane.columnIndex="1"
                  GridPane.rowIndex="2"/>
        <AnchorPane>
            <Button fx:id="execute" layoutX="98.0" layoutY="55.0" mnemonicParsing="false" onAction="#exectue"
                    text="Button"/>
        </AnchorPane>
        <AnchorPane GridPane.rowIndex="1">
            <Label layoutX="115.0" layoutY="50.0" text="Label"/>
        </AnchorPane>
        <AnchorPane GridPane.rowIndex="2">
            <Label layoutX="111.0" layoutY="50.0" text="Label"/>
        </AnchorPane>
        <BorderPane GridPane.columnIndex="1">
            <center>
                <HBox alignment="CENTER">
                    <children>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                        <TextField/>
                    </children>
                </HBox>
            </center>
            <top>
                <Label text="Silo Codes" BorderPane.alignment="CENTER"/>
            </top>
        </BorderPane>
        <BorderPane GridPane.columnIndex="1" GridPane.rowIndex="1">
            <center>
                <HBox alignment="CENTER">
                    <children>
                        <TextField/>
                    </children>
                </HBox>
            </center>
            <top>
                <Label text="Pattern" BorderPane.alignment="CENTER"/>
            </top>
        </BorderPane>
    </GridPane>
</AnchorPane>
